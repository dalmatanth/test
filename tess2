 var tabsr = [];
  function retourner() {
    var tabval = [];

    var i =0;
    for (i = 0; i < document.getElementsByName("aa").length; i++) {
    var  ok=document.getElementsByName("aa")[i].value;
       tabval.push(ok);
	  tabsr.push(document.getElementsByName("aa")[i].src);

  document.getElementsByName("aa")[i].src='bl.jpg';
	document.getElementsByName("aa")[i].id=i;



    }
    // 3-en-1 :) initialisation + condition + incrÃ©mentation
                   // action


  }
var p=0;
var mem = 0;
var mem2=0;
  function verifier(ca) {

				if (p == 0) {

		    p++;
					var o= document.getElementById(ca);
					alert(tabsr[0]);
				o.src=tabsr[ca];
		 	 mem= o.value;
			return;
	  }




			if (p == 1) {

			p=p-1;
				var o2= document.getElementById(ca);
				mem2=o2.value
				if (mem==mem2){ alert("gg");
				o.src=tabsr[ca];

			}

		return;
		}
  }
